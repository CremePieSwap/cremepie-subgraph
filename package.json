{
  "name": "cremepie-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/CremePieSwap/cremepie-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "auth": "graph auth https://api.thegraph.com/deploy/ <access_token>",
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "remove-local": "graph remove sameepsi/quickswap --node http://127.0.0.1:8020/",
    "create-local": "graph create sameepsi/quickswap --node http://127.0.0.1:8020/",
    "deploy-local": "graph deploy sameepsi/quickswap --ipfs https://api.thegraph.com/ipfs/ --node http://127.0.0.1:8020/ --debug",
    "deploy": "graph deploy --access-token 82bbf86e2bcc4b9690fac116827ae320 --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ sameepsi/quickswap",
    "deploy02": "graph deploy --access-token <access_token> --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ sameepsi/quickswap02",
    "deploy03": "graph deploy --access-token d0e3ef499e5447f6ab11f8652e044083 --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ sameepsi/quickswap03",
    "deploy05": "graph deploy --access-token 82bbf86e2bcc4b9690fac116827ae320 --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ sameepsi/quickswap05",
    "deploy06": "graph deploy --access-token 22155bca882a4af792add2cdc940dbf4 --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ sameepsi/quickswap06",
    "deploy-gen": "graph deploy --access-token $GRAPH_TOKEN --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ $GITHUB_USER/$SUBGRAPH_NAME", 
    "deploy-staging": "graph deploy $THE_GRAPH_GITHUB_USER/$THE_GRAPH_SUBGRAPH_NAME /Uniswap --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/",
    "watch-local": "graph deploy graphprotocol/Uniswap --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
